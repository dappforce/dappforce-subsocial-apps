{
  "extends": "./node_modules/@polkadot/dev/config/tsconfig",
  "exclude": [
    "patches"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@dappforce/blogs/": [ "packages/df-blogs/src/" ],
      "@dappforce/blogs/*": [ "packages/df-blogs/src/*" ],
      "@dappforce/types/*": [ "packages/df-types/src/*" ],
      "@dappforce/types/": [ "packages/df-types/src/" ],
      "@polkadot/df-utils/": [ "packages/df-utils/src" ],
      "@polkadot/df-utils/*": [ "packages/df-utils/src/*" ],
      "@polkadot/df-settings/": [ "packages/df-settings/src/" ],
      "@polkadot/df-settings/*": [ "packages/df-settings/src/*" ],

      "@polkadot/app-accounts": [ "packages/app-accounts/src" ],
      "@polkadot/app-accounts/*": [ "packages/app-accounts/src/*" ],
      "@polkadot/apps": [ "packages/apps/src" ],
      "@polkadot/apps/*": [ "packages/apps/src/*" ],
      "@polkadot/ui-api": [ "packages/ui-api/src" ],
      "@polkadot/ui-api/*": [ "packages/ui-api/src/*" ],
      "@polkadot/ui-app": [ "packages/ui-app/src" ],
      "@polkadot/ui-app/*": [ "packages/ui-app/src/*" ],
      "@polkadot/ui-params": [ "packages/ui-params/src" ],
      "@polkadot/ui-params/*": [ "packages/ui-params/src/*" ],
      "@polkadot/ui-reactive": [ "packages/ui-reactive/src" ],
      "@polkadot/ui-reactive/*": [ "packages/ui-reactive/src/*" ],
      "@polkadot/ui-signer": [ "packages/ui-signer/src" ],
      "@polkadot/ui-signer/*": [ "packages/ui-signer/src/*" ]
    },
    "typeRoots": [
      "./node_modules/@polkadot/ts",
      "./node_modules/@types"
    ]
  }
}
